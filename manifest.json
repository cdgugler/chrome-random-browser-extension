{
    "name": "Random Browser",
    "version": "1.0",
    "description": "Randomly Browse the Internets",
    "permissions": ["activeTab", "tabs", "<all_urls>", "alarms"],
    "background":  {
        "scripts": ["background.js"],
        "persistant": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
      }
    },
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "manifest_version": 2
}